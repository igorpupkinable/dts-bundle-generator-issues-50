{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": [
      "DOM",
      "ES2019",
    ],
    "module": "ES2020",
    "moduleResolution": "node",
    "strict": true,
    "target": "ES2019",
  },
}
